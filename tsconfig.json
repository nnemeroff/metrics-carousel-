{
  "compilerOptions": {
    "composite": false,
    "incremental": false,
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ES2019",
    "lib": ["dom", "es2020"],
    "types": ["react", "react-dom"]
  },
  "include": ["src/**/*"]
}


